{
    "name": "ka4ivan/laravel-notification-channel-instagram",
    "description": "Instagram Notifications Channel for Laravel",
    "keywords": [
        "laravel", "notifications", "instagram", "instagram api", "laravel notification channel", "laravel package", "social media", "realtime notifications", "laravel notifications channel", "instagram notifications"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kachula Ivan",
            "email": "ka4ivan.dev@gmail.com",
            "role": "Architect & Developer",
            "homepage": "https://github.com/ka4ivan"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/notifications": "~10.0 || ~11.0 || ~12.0",
        "illuminate/support": "~10.0 || ~11.0 || ~12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Instagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Instagram\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
